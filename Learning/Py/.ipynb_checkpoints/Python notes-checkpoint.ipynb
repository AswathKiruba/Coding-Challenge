    "```\n",
    " fileconnection = open(\"olympics.txt\", 'r')\n",
    " lines = fileconnection.readlines()\n",
    " header = lines[0]\n",
    " field_names = header.strip().split(',')\n",
    " print(field_names)\n",
    " for row in lines[1:]:\n",
    "     vals = row.strip().split(',')\n",
